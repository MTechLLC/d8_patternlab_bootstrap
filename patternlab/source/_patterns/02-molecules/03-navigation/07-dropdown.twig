<div class="{{ dropdownClass}} clearfix">
	<button class="btn {{ buttonClass }} dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
	Dropdown
    {% if caret %}<span class="{{ caret }}"></span>{% endif %}
	</button>
	<ul class="dropdown-menu" aria-labelledby="dropdownMenu1">
    {% for item in dropdownList %}
			<li {% if item.separator %}role="separator"{% endif %} {% if item.listItemClass %} class="{{ item.listItemClass }}"{% endif %}><a href="{{ item.url }}">{{ item.text }}</a></li>
    {% endfor %}
	</ul>
</div>
